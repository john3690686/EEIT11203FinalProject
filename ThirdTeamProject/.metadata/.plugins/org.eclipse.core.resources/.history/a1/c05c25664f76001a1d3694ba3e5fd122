package tw.valckth.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

import tw.valckth.user.model.Profile;
import tw.valckth.user.model.ProfileService;

@Controller
public class ValckthController {
	
	@RequestMapping(path = "/insertProfile")
	public String processAction() {
		ProfileService pService = new ProfileService();
		Profile p = new Profile();
		p.setUserId("Maryid");
		p.setUserName("Mary");
		p.setUserPwd("mary");
		pService.insertProfile(p);
		return "Success.jsp";
	}
}
